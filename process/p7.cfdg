startshape TREE

shape TREE
{
    FIRST []
    FIRST [r 180]
}

shape FIRST
{
    Line []
    Line [flip 90]
    Line [flip -90]
    Line [flip 90]
}

shape Line
rule 98
{
    CIRCLE [b 1]
    CIRCLE [s 0.9 b 1 sat 1]
    Line [y 0.2 s 0.99 rotate 3 h 2]
}


rule 2
{
    CIRCLE [b 1]
    CIRCLE [s 0.9 b 1 sat 1]
    Line [y 0.2 s 0.99 flip 90 h 2]
    Line [s 0.2 b 0.2 y 0.2]
}
